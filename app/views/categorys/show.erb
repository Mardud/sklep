<p>
  <strong>Category Title:</strong>
  <%= @category.title %>
</p>
<h2>Produkty</h2>
<% @category.products.each do |product| %>
    <p>
      <strong>Nazwa:</strong>
      <%= product.name %>
    </p>

    <p>
      <strong>Opis:</strong>
      <%= product.description %>
    </p>

    <p>
      <strong>Cena:</strong>
      <%= product.price %>
    </p>
<% end %>

<%= link_to 'Back', categorys_path %>